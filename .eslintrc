{
	"parser": "babel-eslint",
	"parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
  },
	"extends": "airbnb-base",
	"env": {
		"browser": true,
		"es6": true
	},
	"globals": {
		"Babel": true,
		"React": true,
    "CONFIG": true,
		"UE": true,
		"wx": true,
		"BMap": true
	},
	"plugins": [
		"react",
		"jsx-a11y"
	],
	"rules": {
		"no-debugger":"off",
		"brace-style": [2, "1tbs"],
		"no-tabs":0,
		"no-unused-vars":[2, { "varsIgnorePattern":"React", "args": "after-used" }],
		"dot-notation": 0,
		"prefer-arrow-callback": [2, { "allowNamedFunctions": true }],
		"no-param-reassign": [2, { "props": false }],
		"no-mixed-spaces-and-tabs":0,
		"no-plusplus":0,
		"linebreak-style": 0, 
		"camelcase": 0, 
		"react/jsx-uses-vars": 2,
		"import/imports-first": 0,
		"import/newline-after-import": 0,
		"import/no-dynamic-require": 0,
		"import/no-extraneous-dependencies": 0,
		"import/no-named-as-default": 0,
		"import/no-unresolved": 0,
		"import/prefer-default-export": 0,
		"import/extensions": 0,
		"class-methods-use-this": 0,
		"consistent-return": 0,
		"max-len": [2, {"code": 150}],
		"no-nested-ternary": 0,
		"no-restricted-syntax": 0
	}
}